<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <!-- PawnKinds for Player Colonies and Tribes -->

<PawnKindDef Abstract="True" Name="ProtogenPlayerBase">
	<race>Protogen</race>
	<backstoryFiltersOverride>
        <li>
            <categories>
                <li>ProtogenBackstoriesChildhood</li>
				<li>ProtogenBackstoriesAdulthood</li>
            </categories>
        </li>
    </backstoryFiltersOverride>
    <combatPower>30</combatPower>
    <isFighter>false</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
</PawnKindDef>

  <!-- Space Refugee for refugee events -->

  <PawnKindDef>
	<race>Protogen</race>
	<backstoryFiltersOverride>
        <li>
            <categories>
                <li>ProtogenBackstoriesChildhood</li>
				<li>ProtogenBackstoriesAdulthood</li>
            </categories>
        </li>
    </backstoryFiltersOverride>
    <combatPower>30</combatPower>
    <defName>ProtogenSpaceRefugee</defName>
    <label>Protogen space refugee</label>
    <baseRecruitDifficulty>0.30</baseRecruitDifficulty>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
    <forceNormalGearQuality>true</forceNormalGearQuality>
    <isFighter>false</isFighter>
    <chemicalAddictionChance>0.08</chemicalAddictionChance>
    <gearHealthRange>
      <min>0.4</min>
      <max>1.4</max>
    </gearHealthRange>
    <apparelMoney>
      <min>0</min>
      <max>1000</max>
    </apparelMoney>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <techHediffsMoney>
      <min>50</min>
      <max>800</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.08</techHediffsChance>
    <initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>2~5</initialResistanceRange>
  </PawnKindDef>

